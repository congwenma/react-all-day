{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false, // difficult to maintain
    "interface-name": false, // no i leading interfaces
    "member-access": false, // no public/private
    "no-var-requires": false, // needed for local images
    "curly": false, // does not play well with prettier
    "no-debugger": false
  }
}
